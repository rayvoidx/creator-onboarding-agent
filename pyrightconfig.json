{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "include": ["src", "tests", "config"],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "node_modules",
    "frontend",
    "node"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",
  "reportOptionalMemberAccess": "warning",
  "reportOptionalCall": "warning"
}
