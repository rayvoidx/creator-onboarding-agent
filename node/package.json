{
  "name": "consumer-agent-system-node",
  "version": "0.1.0",
  "private": true,
  "description": "Node.js + TypeScript service for consumer-agent-system",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "prefetch:news": "tsx src/scripts/prefetchNews.ts",
    "openapi:pull": "python ../scripts/export_openapi.py ./src/types/openapi.json",
    "types:gen": "openapi-typescript ./src/types/openapi.json -o ./src/types/openapi.d.ts",
    "types:generate": "npm run openapi:pull && npm run types:gen"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "openai": "^4.56.0",
    "pino": "^9.2.0",
    "rss-parser": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/node": "^20.11.30",
    "openapi-typescript": "^7.5.0",
    "tsx": "^4.15.7",
    "typescript": "^5.6.3"
  }
}
